<!--index.wxml-->
<view class="container">
  <canvas
  style="width: 100%; height: 100%;border:1px;position:absolute;top:0;left:0; solid black;"
  canvas-id="first"
  disable-scroll=true
  bindtouchstart="bindtouchstart"
  bindtouchmove="bindtouchmove"
  bindtouchend="bindtouchend"
  bindtap="bindtap"
  bindlongpress="bindlongpress"
  ></canvas>
</view>
